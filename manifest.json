{
    "name": "novel-experience",
    "version": "1.6.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "webNavigation"
    ]
}